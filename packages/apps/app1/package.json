{
  "name": "app1",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "dependencies": {
    "pm-comps-lib": "~0.0.1",
    "@react-workspaces/react-scripts": "3.0.0-alpha-04",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "core-js": "^3.2.1",
    "node-sass": "7.0.0",
    "regenerator-runtime": "^0.13.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.js --max-warnings=0 --format=codeframe"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie > 10",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/setupTests.{js,jsx}",
      "!src/index.{js,jsx}",
      "!src/serviceWorker.{js,jsx}",
      "!src/**/*.(spec|test|stories).{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 98,
        "functions": 98,
        "lines": 98
      }
    }
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "cross-env": "6.0.3",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "npm-force-resolutions": "0.0.3"
  }
}
